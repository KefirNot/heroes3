{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": "./",
        "paths": {
            "helper/*": [
                "src/helper/*"
            ],
            "component/*": [
                "src/component/*"
            ],
            "store/*": [
                "src/store/*"
            ],
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}